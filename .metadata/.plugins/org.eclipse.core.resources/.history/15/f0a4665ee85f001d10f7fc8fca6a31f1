package test;


class Abc
{
	
	int roll_no;
	String name;
	
	public int getRoll_no() {
		return roll_no;
	}
	public void setRoll_no(int roll_no) {
		this.roll_no = roll_no;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}

}
public class EncapsulationDemo {

	public static void main(String[] args) {
	
		Abc obj = new Abc();
		obj.setName("Madhuri");
		obj.getName();
		obj.setRoll_no(102);
		obj.getRoll_no();
	}

}
