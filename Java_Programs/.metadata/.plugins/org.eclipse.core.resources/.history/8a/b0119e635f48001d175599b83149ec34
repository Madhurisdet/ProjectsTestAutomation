
public class RemoveJunk {

	public static void main(String[] args) {
		
		
	String s= "!@#$%^&@@!@#*~";
	
	s=s.replaceAll("[^a-zA-Z0-9]", "");
	String s1="Madhuri@#$Bhivaji1234%@$##$@@@@@@Waghmode%$#@";
	s1=s1.replaceAll("[^a-zA-Z0-9]", "");
	
	System.out.println(s);
	System.out.println(s1);
	}

}
